  <script>
    function getParameterByName(name) {
      name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
      var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
          results = regex.exec(location.href);
      return results === null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }

    function validate(formData, jqForm, options) {
      return true;
    }

    function success(responseText, statusText, xhr, $form)  {
      $('#contact-us').hide();
      $('#say-hello').hide();
      $('#contact-confirm').show();
    }

    function error(responseText, statusText, xhr, $form)  {
      alert('error');
    }

    function setupNav() {
      console.log('setupNav');
      var winHeight = $(window).height();
	    var winWidth = $(window).width();

      if (isGoogle) {
        return;
      }

      $('.masthead').css({height: winHeight+'px'});

      $('#nav').affix({
        offset: {
          top: $('header').height()
        }
      });
    }

    $(document).ready(function() {
      Greenfield.init().initMaps();
      Grid().init();
      setupNav();

      $('body').scrollspy({ target: '.navbar-nav' })

      $('#contact-form').validate();

      $('#contact-form').ajaxForm({
        beforeSubmit: validate,
        success: success,
        error: error
      });
    });

    $(window).resize(function(){
      setupNav();
    });

    $(window).load(function(){
        var anchor = getParameterByName('anchor');
        $('#' + anchor + '-link').click();
    });
  </script>

